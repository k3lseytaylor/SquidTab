{
  "manifest_version": 2,

  "name": "SquidTabs",
  "description": "",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
 },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "http://*/*", 
    "https://*/*",
    "background"
  ]
}
