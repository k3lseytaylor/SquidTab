{
  "manifest_version": 2,

  "name": "SquidTab",
  "description": "",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "background",
    "activeTab",
    "storage",
    "tabs",
  "http://*/*", 
  "https://*/*"
  ]
}
